{
    "name": "huh",
    "displayName": "huh",
    "description": "Syntax highlighter for huh (language by onmert)",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.64.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "huh",
            "aliases": ["huh", "huh"],
